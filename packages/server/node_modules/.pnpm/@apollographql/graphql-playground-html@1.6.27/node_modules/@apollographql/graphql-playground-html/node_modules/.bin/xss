#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/xss@1.0.8/node_modules/xss/bin/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/xss@1.0.8/node_modules/xss/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/xss@1.0.8/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules:/Users/godoy/Documents/client-server-example/packages/node_modules:/Users/godoy/Documents/client-server-example/node_modules:/Users/godoy/Documents/node_modules:/Users/godoy/node_modules:/Users/node_modules:/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/@apollographql/graphql-playground-html@1.6.27/node_modules/xss/bin/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/@apollographql/graphql-playground-html@1.6.27/node_modules/xss/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/@apollographql/graphql-playground-html@1.6.27/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/@apollographql/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/@apollographql/graphql-playground-html@1.6.27/node_modules/@apollographql/graphql-playground-html/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/@apollographql/graphql-playground-html@1.6.27/node_modules/@apollographql/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../xss/bin/xss" "$@"
else
  exec node  "$basedir/../../../../xss/bin/xss" "$@"
fi
