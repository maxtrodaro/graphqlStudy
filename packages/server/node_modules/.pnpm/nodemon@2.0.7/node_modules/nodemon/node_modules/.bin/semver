#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/semver@5.7.1/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules:/Users/godoy/Documents/client-server-example/packages/node_modules:/Users/godoy/Documents/client-server-example/node_modules:/Users/godoy/Documents/node_modules:/Users/godoy/node_modules:/Users/node_modules:/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/nodemon@2.0.7/node_modules/semver/bin/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/nodemon@2.0.7/node_modules/semver/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/nodemon@2.0.7/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/nodemon@2.0.7/node_modules/nodemon/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../semver/bin/semver" "$@"
else
  exec node  "$basedir/../../../semver/bin/semver" "$@"
fi
