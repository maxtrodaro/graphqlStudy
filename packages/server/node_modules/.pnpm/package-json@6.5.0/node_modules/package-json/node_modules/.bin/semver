#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/semver@6.3.0/node_modules/semver/bin/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/semver@6.3.0/node_modules/semver/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/semver@6.3.0/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules:/Users/godoy/Documents/client-server-example/packages/node_modules:/Users/godoy/Documents/client-server-example/node_modules:/Users/godoy/Documents/node_modules:/Users/godoy/node_modules:/Users/node_modules:/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/package-json@6.5.0/node_modules/semver/bin/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/package-json@6.5.0/node_modules/semver/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/package-json@6.5.0/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/package-json@6.5.0/node_modules/package-json/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../../../semver/bin/semver.js" "$@"
fi
