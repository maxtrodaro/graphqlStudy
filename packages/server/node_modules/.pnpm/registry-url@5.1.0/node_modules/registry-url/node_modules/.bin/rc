#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/rc@1.2.8/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules:/Users/godoy/Documents/client-server-example/packages/node_modules:/Users/godoy/Documents/client-server-example/node_modules:/Users/godoy/Documents/node_modules:/Users/godoy/node_modules:/Users/node_modules:/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/registry-url@5.1.0/node_modules/rc/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/registry-url@5.1.0/node_modules:/Users/godoy/Documents/client-server-example/packages/server/node_modules/.pnpm/registry-url@5.1.0/node_modules/registry-url/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../rc/cli.js" "$@"
else
  exec node  "$basedir/../../../rc/cli.js" "$@"
fi
